<resources>
    <!-- Base application theme. -->
    <style name="Theme.MasterDarkMode" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="anyColor">@color/anyColor</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MasterDarkMode.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MasterDarkMode.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MasterDarkMode.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>